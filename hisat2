stringtie -e -B -p 8 -G stringtie_merged.gtf -o ballgown/Pt1_Pre_M_hisat2.gtf/Pt1_Pre_M_hisat2.gtf Pt1_Pre_M_hisat2.bam

fpkm = data.frame(geneNames=ballgown::geneNames(bg), geneIDs=ballgown::geneIDs(bg), fpkm)

